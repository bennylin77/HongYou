.project_section_image_container{
  width: 100%;
  //height: 300px;
  //padding-bottom: 70% ; /* % of width, defines aspect ratio*/
  position: relative;
  //margin: 0 auto;
  overflow: hidden;
  visibility: hidden;
}
.project_section_image{
  width: 100%;
  height: 350px;
  margin: 0 auto;
  background-attachment: scroll; 
  background-position: center center;
  background-size: cover;  
  transition: all 1s; 
  -moz-transition: all 1s; 
  -webkit-transition: all 1s; 
  -o-transition: all 1s;   
  &:before {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    //background: rgba(33, 33, 33, 1);
    //opacity: .4; 
  }  
}
.project_section_image_large{
  width: 100%;
  height: 716px;
  margin: 0 auto;
  background-attachment: scroll; 
  background-position: center center;
  background-size: cover;  
  transition: all 1s; 
  -moz-transition: all 1s; 
  -webkit-transition: all 1s; 
  -o-transition: all 1s;   
  &:before {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: rgba(33, 33, 33, 1);
    opacity: .4; 
  }  
}
.no-touch .project_section_image:hover, 
.no-touch .project_section_image_large:hover{ 
  transform: scale(1.1); 
  -ms-transform: scale(1.1); 
  -webkit-transform: scale(1.1); 
  -o-transform: scale(1.1); 
  -moz-transform: scale(1.1); 
  &:before {
    background: none;
  }
}
