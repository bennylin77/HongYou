.index_top_container .top_1998_container{
    margin: 0 auto;
    width: 300px;
    top: 0px;
    //visibility: hidden;   
    position: relative;     
}
.top_1998_container h2.no-background {
    position: relative;
    overflow: hidden;  
    .text {
        display: inline-block;
        vertical-align: baseline;
        zoom: 1;
        *display: inline;
        *vertical-align: auto;
        position: relative;
        padding: 0 20px;
        font-size: 18px;
        letter-spacing: 2px;
        &:before, &:after {
            content: '';
            display: block;
            width: 1000px;
            position: absolute;
            top: 0.5em;
            border-top: 1px solid #ffffff;
        }
        &:before { right: 100%; }
        &:after { left: 100%; }
    }
}
