  margin-top: 30px; 
  height: 60px;  
}
#hexagon {
  width: 80px;
  height: 30px;
  background: #ffffff;
  position: relative;
    z-index:1;
    margin:0 auto;
}
#hexagon:before {
  content: "";
  left:-80px;
  display:block;
    position: relative;
    border-right: 10px solid #ffffff;
    border-top: 15px solid transparent;
    border-bottom: 15px solid transparent;
}
#hexagon:after {
  content: "";
    right:-80px;
    top:-30px;
    position: relative;
    display:block;
  border-left: 10px solid #ffffff;
  border-top: 15px solid transparent;
  border-bottom: 15px solid transparent;
}
.hex-over {
  color: rgba(45, 72, 139, 1);
  z-index:5;
  position:relative;
  width:100%;
  top:-25px;
  text-align:center;
  padding:0px 0px 1px 0px;
}
